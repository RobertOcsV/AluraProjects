    <MudCardMedia Image="images/cardArtista.png"></MudCardMedia>
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.h6">@Musica!.Nome</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent>
        <MudText Typo="Typo.body2">Artista/Banda: @Musica!.NomeArtista</MudText>
